{
  "users": [
    {
      "name": "admin",
      "password": "admin123",
      "tags": "administrator"
    },
    {
      "name": "app_user",
      "password": "app123",
      "tags": ""
    }
  ],
  "vhosts": [
    {
      "name": "/"
    },
    {
      "name": "/app"
    }
  ],
  "permissions": [
    {
      "user": "admin",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    },
    {
      "user": "admin",
      "vhost": "/app",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    },
    {
      "user": "app_user",
      "vhost": "/app",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "exchanges": [
    {
      "name": "app.events",
      "vhost": "/app",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "queues": [
    {
      "name": "app.notifications",
      "vhost": "/app",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 86400000,
        "x-max-length": 10000
      }
    }
  ],
  "bindings": [
    {
      "source": "app.events",
      "vhost": "/app",
      "destination": "app.notifications",
      "destination_type": "queue",
      "routing_key": "notification.*",
      "arguments": {}
    }
  ]
}
